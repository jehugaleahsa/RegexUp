<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.0.1</Version>
    <Description>Compose, decompose and analyze regular expressions.</Description>
    <Copyright>Truncon Â© 2018</Copyright>
    <PackageLicenseUrl>https://github.com/jehugaleahsa/RegexUp/blob/master/UNLICENSE.md</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/jehugaleahsa/RegexUp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/jehugaleahsa/RegexUp.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>regex parse build compose analyze</PackageTags>
    <PackageReleaseNotes>Experimental release</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
